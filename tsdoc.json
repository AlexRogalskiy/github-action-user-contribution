{
    "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
    "tagDefinitions": [
        {
            "tagName": "@author",
            "syntaxKind": "block",
            "allowMultiple": true
        },
        {
            "tagName": "@param",
            "syntaxKind": "block"
        },
        {
            "tagName": "@description",
            "syntaxKind": "block"
        },
        {
            "tagName": "@implements",
            "syntaxKind": "modifier"
        }
    ],
    "supportForTags": {
        "@author": true,
        "@param": true,
        "@description": true,
        "@implements": true
    }
}
